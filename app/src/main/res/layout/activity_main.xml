<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Add_profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B8F8E3"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/add_profile"
        android:layout_width="201dp"
        android:layout_height="45dp"
        android:layout_marginBottom="504dp"
        android:text="Add Profiles"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/unblocker"
        android:layout_width="201dp"
        android:layout_height="45dp"
        android:text="Unblock"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_status"
        app:layout_constraintVertical_bias="0.117" />

    <Button
        android:id="@+id/view_profile"
        android:layout_width="201dp"
        android:layout_height="45dp"
        android:layout_marginBottom="300dp"
        android:text="View Profiles"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_profile"
        app:layout_constraintVertical_bias="0.226" />

    <Button
        android:id="@+id/check_status"
        android:layout_width="201dp"
        android:layout_height="45dp"
        android:text="Check Status"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_profile"
        app:layout_constraintVertical_bias="0.107" />

</androidx.constraintlayout.widget.ConstraintLayout>